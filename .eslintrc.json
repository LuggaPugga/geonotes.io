{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "next"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "eqeqeq": "warn",
        "no-console": "warn",
        "no-redeclare": "warn",
        "no-unreachable": "warn"
    }
}
